<Window x:Class="ftdicomm.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ftdicomm"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="300"
        Closed="Window_Closed">
    <Grid>
        <StackPanel>
            <TextBlock x:Name="tbPropList" Margin="3">
                
            </TextBlock>
            <Button x:Name="btnAsyncCycle" Margin="3" Click="BtnAsyncCycle_Click">
                Show prop
            </Button>
            <Button x:Name="btnStop" Margin="3" Click="BtnStop_Click">
                Stop
            </Button>
            <Button x:Name="btnTest" Margin="3" Click="BtnTest_Click">
                Test
            </Button>
        </StackPanel>
    </Grid>
</Window>
